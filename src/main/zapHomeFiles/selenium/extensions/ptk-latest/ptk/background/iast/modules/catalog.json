{
    "version": 1,
    "engine": "IAST",
    "hooks": {
        "hook.dom.htmlStrings": {
            "id": "hook.dom.htmlStrings",
            "title": "DOM HTML string sinks",
            "description": "HTML string insertion sinks (innerHTML/outerHTML/insertAdjacentHTML/document.write) with inline event scanning.",
            "coversSinks": [
                "dom.innerHTML",
                "dom.outerHTML",
                "dom.insertAdjacentHTML",
                "document.write",
                "nav.iframe.srcdoc",
                "dom.inline_event"
            ],
            "cost": "medium",
            "defaultEnabled": true
        },
        "hook.dom.mutations": {
            "id": "hook.dom.mutations",
            "title": "DOM mutations and inserted nodes",
            "description": "Node insertion sinks (appendChild/insertBefore/etc) with attribute traversal.",
            "coversSinks": [
                "dom.mutation",
                "dom.inline_event",
                "script.element.src"
            ],
            "cost": "high",
            "defaultEnabled": true
        },
        "hook.dom.attributes": {
            "id": "hook.dom.attributes",
            "title": "DOM attribute assignments",
            "description": "Attribute/property setters on existing nodes (setAttribute, href/src/onclick assignments).",
            "coversSinks": [
                "dom.attr.href",
                "dom.attr.src",
                "dom.attr.action",
                "dom.attr.formaction",
                "dom.inline_event",
                "http.image.src",
                "nav.iframe.src",
                "nav.iframe.srcdoc",
                "nav.location.href",
                "script.element.src",
                "document.domain"
            ],
            "cost": "medium",
            "defaultEnabled": true
        },
        "hook.code.exec": {
            "id": "hook.code.exec",
            "title": "Dynamic code execution",
            "description": "eval, Function constructor, and string-based timers.",
            "coversSinks": [
                "code.eval",
                "code.function.constructor",
                "code.function.apply",
                "code.setTimeout",
                "code.setInterval"
            ],
            "cost": "low",
            "defaultEnabled": true
        },
        "hook.nav.redirects": {
            "id": "hook.nav.redirects",
            "title": "Navigation and redirects",
            "description": "location.href/assign/replace and window.open redirects.",
            "coversSinks": [
                "nav.location.href",
                "nav.location.assign",
                "nav.location.replace",
                "nav.history.pushState",
                "nav.navigation.navigate",
                "nav.window.open"
            ],
            "cost": "low",
            "defaultEnabled": true
        },
        "hook.net.exfil": {
            "id": "hook.net.exfil",
            "title": "Network exfiltration",
            "description": "Fetch/XHR/sendBeacon/WebSocket/RTC data egress.",
            "coversSinks": [
                "csrf.fetch",
                "http.fetch.url",
                "http.fetch.headers",
                "http.xhr.open",
                "http.xhr.send",
                "http.xhr.setRequestHeader",
                "http.navigator.sendBeacon",
                "http.image.src",
                "clipboard.writeText",
                "realtime.websocket.send",
                "realtime.webrtc.send"
            ],
            "cost": "medium",
            "defaultEnabled": true
        },
        "hook.client.json": {
            "id": "hook.client.json",
            "title": "Client-side JSON parsing",
            "description": "JSON.parse sink detection.",
            "coversSinks": [
                "client.json.parse"
            ],
            "cost": "low",
            "defaultEnabled": true
        },
        "hook.storage": {
            "id": "hook.storage",
            "title": "Storage and cookies",
            "description": "Storage writes and cookie persistence; storage/cookie sources.",
            "coversSinks": [
                "storage.localStorage.setItem",
                "storage.sessionStorage.setItem",
                "storage.document.cookie"
            ],
            "producesSources": [
                "localStorage",
                "sessionStorage",
                "cookie"
            ],
            "cost": "low",
            "defaultEnabled": true
        },
        "hook.postMessage": {
            "id": "hook.postMessage",
            "title": "postMessage and channels",
            "description": "postMessage, BroadcastChannel, and MessagePort sinks; message data sources.",
            "coversSinks": [
                "postmessage.anyOrigin",
                "postmessage.crossOrigin",
                "channel.broadcast.postMessage",
                "channel.messageport.postMessage"
            ],
            "producesSources": [
                "postMessage"
            ],
            "cost": "low",
            "defaultEnabled": true
        },
        "hook.console.leaks": {
            "id": "hook.console.leaks",
            "title": "Console leakage",
            "description": "console.log/error sinks.",
            "coversSinks": [
                "log.console.log",
                "log.console.error"
            ],
            "cost": "low",
            "defaultEnabled": true
        },
        "hook.script.loading": {
            "id": "hook.script.loading",
            "title": "Script loading",
            "description": "ServiceWorker and Worker script loading sinks.",
            "coversSinks": [
                "worker.serviceWorker.register",
                "worker.webworker.constructor",
                "script.element.src"
            ],
            "cost": "low",
            "defaultEnabled": true
        },
        "hook.sanitizers": {
            "id": "hook.sanitizers",
            "title": "Sanitizer detection",
            "description": "Allow-listed sanitizers for taint confidence adjustments.",
            "producesSanitizers": [
                "san.domPurify"
            ],
            "cost": "low",
            "defaultEnabled": false
        }
    },
